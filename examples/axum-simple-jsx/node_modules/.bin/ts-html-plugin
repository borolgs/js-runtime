#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ol/Developer/prsnl/js-runtime/node_modules/.pnpm/@kitajs+ts-html-plugin@4.1.1_@kitajs+html@4.2.9_typescript@5.8.3/node_modules/@kitajs/ts-html-plugin/dist/node_modules:/Users/ol/Developer/prsnl/js-runtime/node_modules/.pnpm/@kitajs+ts-html-plugin@4.1.1_@kitajs+html@4.2.9_typescript@5.8.3/node_modules/@kitajs/ts-html-plugin/node_modules:/Users/ol/Developer/prsnl/js-runtime/node_modules/.pnpm/@kitajs+ts-html-plugin@4.1.1_@kitajs+html@4.2.9_typescript@5.8.3/node_modules/@kitajs/node_modules:/Users/ol/Developer/prsnl/js-runtime/node_modules/.pnpm/@kitajs+ts-html-plugin@4.1.1_@kitajs+html@4.2.9_typescript@5.8.3/node_modules:/Users/ol/Developer/prsnl/js-runtime/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/ol/Developer/prsnl/js-runtime/node_modules/.pnpm/@kitajs+ts-html-plugin@4.1.1_@kitajs+html@4.2.9_typescript@5.8.3/node_modules/@kitajs/ts-html-plugin/dist/node_modules:/Users/ol/Developer/prsnl/js-runtime/node_modules/.pnpm/@kitajs+ts-html-plugin@4.1.1_@kitajs+html@4.2.9_typescript@5.8.3/node_modules/@kitajs/ts-html-plugin/node_modules:/Users/ol/Developer/prsnl/js-runtime/node_modules/.pnpm/@kitajs+ts-html-plugin@4.1.1_@kitajs+html@4.2.9_typescript@5.8.3/node_modules/@kitajs/node_modules:/Users/ol/Developer/prsnl/js-runtime/node_modules/.pnpm/@kitajs+ts-html-plugin@4.1.1_@kitajs+html@4.2.9_typescript@5.8.3/node_modules:/Users/ol/Developer/prsnl/js-runtime/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@kitajs/ts-html-plugin/dist/cli.js" "$@"
else
  exec node  "$basedir/../@kitajs/ts-html-plugin/dist/cli.js" "$@"
fi
